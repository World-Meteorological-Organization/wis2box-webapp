          "dc:description": "WMO station number",
          "datatype": {"base": "integer"}
        },
        {
          "titles": "station_type",
          "dc:description": "Type of observing station, encoding using code table 0 02 001 (set to 0, automatic)",
          "datatype": {"base": "integer"}
        },
        {
